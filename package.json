{
  "name": "demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --port=4201",
    "build": "ng build --aot",
    "lib:build": "rimraf dist && ng build ng-wizard --prod",
    "lib:themes": "mkdir dist\\ng-wizard\\themes && copy src\\ng-wizard\\themes\\*.css dist\\ng-wizard\\themes",
    "lib:files": "copy README.md dist\\ng-wizard && copy LICENSE dist\\ng-wizard",
    "lib:pack": "cd dist/ng-wizard && npm pack",
    "lib": "npm run lib:build && npm run lib:themes && npm run lib:files && npm run lib:pack",
    "test": "ng test ng-wizard",
    "lint": "ng lint ng-wizard",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "bootstrap": "^5.1.1",
    "core-js": "^3.35.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.10",
    "@angular/cli": "~17.0.0",
    "@angular/compiler-cli": "~17.0.0",
    "@angular/language-service": "~17.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/jasminewd2": "^2.0.13",
    "@types/node": "12.11.1",
    "codelyzer": "^6.0.2",
    "jasmine-core": "^5.1.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "protractor": "^7.0.0",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "typescript": "~5.2.2",
    "ng-packagr": "^17.0.3"
