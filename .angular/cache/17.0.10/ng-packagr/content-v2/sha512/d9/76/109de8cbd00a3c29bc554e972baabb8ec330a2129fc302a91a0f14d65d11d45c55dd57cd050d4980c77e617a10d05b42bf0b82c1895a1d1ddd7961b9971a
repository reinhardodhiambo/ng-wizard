unction merge(target, source) {\n    // Iterate through `source` properties and if an `Object` set property to merge of `target` and `source` properties\n    for (let key of Object.keys(source)) {\n        if (source[key] instanceof Object && key in target) {\n            Object.assign(source[key], merge(target[key], source[key]));\n        }\n    }\n    // Join `target` and modified `source`\n    Object.assign(target || {}, source);\n    return target;\n}\n//# sourceMappingURL=data:application/json;base64,","customTransformCache":false,"dependencies":[],"id":"/home/reinhard/ng-wizard/dist/ng-wizard/esm2022/lib/utils/functions.mjs","meta":{},"moduleSideEffects":true,"originalCode":"// https://gist.github.com/ahtcx/0cd94e62691f539160b32ecda18af3d6\n// Merge a `source` object to a `target` recursively\nexport function merge(target, source) {\n    // Iterate through `source` properties and if an `Object` set property to merge of `target` and `source` properties\n    for (let key of Object.keys(source)) {\n        if (source[key] instanceof Object && key in target) {\n            Object.assign(source[key], merge(target[key], source[key]));\n        }\n    }\n    // Join `target` and modified `source`\n    Object.assign(target || {}, source);\n    return target;\n}\n//# sourceMappingURL=data:application/json;base64,","originalSourcemap":{"version":3,"file":"functions.js","sourceRoot":"","sources":["../../../../../src/ng-wizard/lib/utils/functions.ts"],"names":[],"mappings":[[[0,0,0,0],[65,0,0,65]],[[0,0,1,0],[52,0,1,52]],[[0,0,2,0],[6,0,2,6],[16,0,2,16],[21,0,2,21],[22,0,2,22],[28,0,2,33],[30,0,2,35],[36,0,2,46]],[[4,0,3,4],[119,0,3,119]],[[4,0,4,4],[9,0,4,9],[13,0,4,13],[16,0,4,16],[20,0,4,20],[26,0,4,26],[27,0,4,27],[31,0,4,31],[32,0,4,32],[38,0,4,38],[39,0,4,39],[41,0,4,41]],[[8,0,5,8],[12,0,5,12],[18,0,5,18],[19,0,5,19],[22,0,5,22],[23,0,5,23],[35,0,5,35],[41,0,5,41],[45,0,5,45],[48,0,5,48],[52,0,5,52],[58,0,5,58],[60,0,5,60]],[[12,0,6,12],[18,0,6,18],[19,0,6,19],[25